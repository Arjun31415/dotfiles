/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @xbg;
    text-color:             @xfg;
	font:				 	"Fantasque Sans Mono 10";
}

window {
    width:      			225px;
    padding:    			25px;
    border:					1px;
    border-radius:          0px;
    border-color:           @x1;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				@x4;
}
